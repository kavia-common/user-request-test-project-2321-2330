# All keys must be prefixed with REACT_APP_ for CRA to expose them at build time

# Backend/API
REACT_APP_API_BASE=https://api.example.com
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=

# Runtime/Build
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=false
REACT_APP_ENABLE_SOURCE_MAPS=true

# Server hints
# Note: CRA dev server uses PORT (without REACT_APP_), typically 3000.
# This value is informational for the UI only.
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false

# Logging
REACT_APP_LOG_LEVEL=info

# Health
REACT_APP_HEALTHCHECK_PATH=/healthz

# Features
REACT_APP_FEATURE_FLAGS={"exampleFlag":true}
REACT_APP_EXPERIMENTS_ENABLED=false
